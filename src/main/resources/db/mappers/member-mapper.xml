<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sdm.spring.mybatis.mappers.MemberMapper">

    <select id="findOne" parameterType="int" resultType="com.sdm.spring.mybatis.models.MemberVO">
        SELECT
        *
        FROM
        member
        WHERE
        code = #{code}
    </select>

</mapper>
